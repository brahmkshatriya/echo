<?xml version="1.0" encoding="utf-8"?>
<dev.brahmkshatriya.echo.utils.ui.custom.ShimmerLayoutSelf xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical">

        <include layout="@layout/skeleton_item_category" />

        <FrameLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:alpha="0.66">

            <include layout="@layout/skeleton_item_track" />
        </FrameLayout>

        <FrameLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:alpha="0.33">

            <include layout="@layout/skeleton_item_category" />
        </FrameLayout>

    </LinearLayout>
</dev.brahmkshatriya.echo.utils.ui.custom.ShimmerLayoutSelf>
